{
    "collectCoverage": true,
    "collectCoverageFrom": [
        "**/*.{js,jsx}",
        "!**/node_modules/**",
        "!**/vendor/**",
        "!**/SES/**",
        "!**/Cloudfront/**",
        "!**/Alexa/**",
        "!**/fixtures/**",
        "!**/coverage/**"
      ],
    "coverageDirectory": "./coverage/",
    "coveragePathIgnorePatterns": ["/node_modules/"],
    "coverageThreshold": {
        "global": {
          "branches": 95,
          "functions": 95,
          "lines": 95,
          "statements": 95
        }
      }
}