{
    "collectCoverage": true,
    "collectCoverageFrom": [
        "**/*.{js,jsx}",
        "!**/node_modules/**",
        "!**/vendor/**"
      ],
    "coverageDirectory": "./coverage/",
    "coveragePathIgnorePatterns": ["/node_modules/"],
    "coverageThreshold": {
        "global": {
          "branches": 1,
          "functions": 1,
          "lines": 1,
          "statements": 1
        }
      }
}