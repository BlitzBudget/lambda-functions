{
    "collectCoverage": true,
    "collectCoverageFrom": [
        "**/*.{js,jsx}",
        "!**/node_modules/**",
        "!**/vendor/**",
        "!**/SES/**",
        "!**/Cloudfront/**",
        "!**/Alexa/**",
        "!**/fixtures/**",
        "!**/coverage/**"
      ],
    "coverageDirectory": "./coverage/",
    "coveragePathIgnorePatterns": ["/node_modules/"],
    "coverageThreshold": {
        "global": {
          "branches": 90,
          "functions": 90,
          "lines": 90,
          "statements": 90
        }
      }
}